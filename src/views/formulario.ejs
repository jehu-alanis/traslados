<!--Subtotal-->
<script type="text/javascript">
  function YY(num) 
  { return +(Math.round(num + "e+2")  + "e-2");
  }

x=1.16;

  function cal() {
  try {
    /////////subtotal/////////////
    var a = parseFloat(document.formulario.ajusteIssste.value).toFixed(2),
        b = x;
        sub=a / b;



 document.formulario.subtotal.value=YY(sub) ; 
  ////////////Diferecia//////////
 var t = parseFloat(document.formulario.valor.value),
       y = parseFloat(document.formulario.ajusteIssste.value);
 dif=t-y;
 document.formulario.diferencia.value = dif.toFixed(2); 
///////////////////impuesto/////////////////

  c = sub,     
 r= 0.16;
imp=c*r;
 document.formulario.impuesto.value = YY(imp) ; 
 

} catch (e) {
  }
}
 </script>




